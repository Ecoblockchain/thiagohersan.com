if page.github || page.vimeo || page.pics || page.astros || page.www || page.collab || page.email
  #social-footer.social-footer-container
    if page.github
      .social-icon
        a(href="#{page.github}" class="fa fa-github" target="_blank")
    if page.vimeo
      .social-icon
        a(href="#{page.vimeo}" class="fa fa-vimeo" target="_blank")
    if page.pics
      .social-icon
        a(href="#{page.pics}" class="fa fa-camera" target="_blank")
    if page.astros
      .social-icon
        a(href="#{page.astros}" class="fo-asterisk" target="_blank")
    if page.www
      .social-icon
        a(href="#{page.www}" class="fa fa-anchor" target="_blank")
    if page.collab
      .social-icon
        a(href="#{page.collab}" class="fa fa-heart" target="_blank")
    if page.email
      label#emailLabel.social-icon(for="emailCheck")
        span(class="fa fa-envelope email-icon")
  input#emailCheck(type="checkbox")
  img#email-img(src="/images/contact/contact_email_wb.png")
